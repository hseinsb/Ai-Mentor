{
  "name": "ai-mentor-functions",
  "version": "1.0.0",
  "description": "Cloud Functions for AI Mentor App",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.3.1",
    "openai": "^4.20.1",
    "puppeteer": "^21.5.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^18.17.0",
    "typescript": "^4.9.5"
  },
  "private": true
}
